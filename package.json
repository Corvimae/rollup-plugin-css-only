{
  "name": "rollup-plugin-css",
  "version": "0.0.1",
  "description": "Rollup plugin that bundles imported css",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c -f cjs -o dist/index.cjs.js && rollup -c -f es -o dist/index.es.js",
    "dev": "rollup -cw -f cjs -o dist/index.cjs.js",
    "lint": "standard rollup.config.js src/**",
    "prepublish": "npm run build"
  },
  "keywords": [
    "rollup-plugin",
    "css",
    "modules",
    "vue2"
  ],
  "license": "MIT",
  "author": "Thomas Ghysels <info@thomasg.be>",
  "homepage": "https://github.com/thgh/rollup-plugin-css",
  "bugs": {
    "url": "https://github.com/thgh/rollup-plugin-css/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thgh/rollup-plugin-css"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "rollup-pluginutils": "^1.5.2"
  },
  "devDependencies": {
    "rollup": "^0.35.10",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-watch": "^2.5.0"
  }
}
